---
title: Interface Diagram - Server Admin
config:
  theme: base
---
flowchart TB
    %% Entry Point
    A["<<window>><br/>Administrator<br/>Login"]
    
    A -->|"Login<br/>Dashboard"| B
    
    B["<<window>><br/>Admin<br/>Dashboard"]

    B -->|"Select<br/>Audit"| F
    B -->|"Select<br/>Health"| G

    %% Audit Log Column
    subgraph AuditGroup [" "]
        direction TB
        F["<<window>><br/>Audit Log<br/>Viewer"]
        F1["<<report>><br/>Filter<br/>Logs"]
        F2["<<report>><br/>View Event<br/>Details"]
        F3["<<report>><br/>Export<br/>Logs"]
    end
    
    F -->|"Select<br/>Filter"| F1
    F -->|"Select<br/>View"| F2
    F -->|"Select<br/>Export"| F3
    
    F1 -->|"Return<br/>Audit"| F
    F2 -->|"Return<br/>Audit"| F
    F3 -->|"Return<br/>Audit"| F
    F -->|"Return<br/>Dashboard"| B

    %% Health Monitor Column
    subgraph HealthGroup [" "]
        direction TB
        G["<<window>><br/>System Health<br/>Monitor"]
        G1["<<report>><br/>Service<br/>Status"]
        G2["<<report>><br/>Latency/Error<br/>Metrics"]
        G3["<<report>><br/>Registry<br/>Status"]
    end
    
    G -->|"Select<br/>Status"| G1
    G -->|"Select<br/>Metrics"| G2
    G -->|"Select<br/>Registry"| G3
    
    G1 -->|"Return<br/>Health"| G
    G2 -->|"Return<br/>Health"| G
    G3 -->|"Return<br/>Health"| G
    G -->|"Return<br/>Dashboard"| B
