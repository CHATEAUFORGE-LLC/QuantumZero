---
title: Interface Diagram - Server Admin (Audit + Health)
config:
  theme: base
---
%% Window Navigation Diagram (concise, vertical)
flowchart TB
    classDef window fill:#cfe8c3,stroke:#2f4f2f,stroke-width:1.5px
    classDef report fill:#e7f5e1,stroke:#2f4f2f,stroke-width:1.2px

    Login["<<window>><br>Administrator Login"]:::window
    Dashboard["<<window>><br>Admin Dashboard"]:::window

    AuditViewer["<<window>><br>Audit Log Viewer"]:::window
    FilterLogs["<<report>><br>Filter Logs"]:::report
    ViewDetails["<<report>><br>View Event Details"]:::report
    ExportLogs["<<report>><br>Export Logs"]:::report

    Health["<<window>><br>System Health Monitor"]:::window
    ServiceStatus["<<report>><br>Service Status"]:::report
    Metrics["<<report>><br>Latency/Error Metrics"]:::report
    RegistryStatus["<<report>><br>Registry Status"]:::report

    Login -->|"Login"| Dashboard

    Dashboard -->|"Select"| AuditViewer
    Dashboard -->|"Select"| Health

    AuditViewer -->|"Select"| FilterLogs
    AuditViewer -->|"Select"| ViewDetails
    AuditViewer -->|"Select"| ExportLogs
    FilterLogs -->|"Back"| AuditViewer
    ViewDetails -->|"Back"| AuditViewer
    ExportLogs -->|"Back"| AuditViewer
    AuditViewer -->|"Back"| Dashboard

    Health -->|"Select"| ServiceStatus
    Health -->|"Select"| Metrics
    Health -->|"Select"| RegistryStatus
    ServiceStatus -->|"Back"| Health
    Metrics -->|"Back"| Health
    RegistryStatus -->|"Back"| Health
    Health -->|"Back"| Dashboard
